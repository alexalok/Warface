<ammo name="bullet_missile_ai" class="HomingMissile">
	<icons>
		<combatlog icon="GrenadeLauncher"/>
	</icons>
	<geometry>
		<firstperson name="Objects\props\missile\stinger.cgf" angles="-90, 0, 0"/>
	</geometry>
	<trail>
		<param name="effect" value="rocket_trails.trail_ai"/>
		<param name="prime" value="false"/>
	</trail>
	<flags>
		<param name="serverspawn" value="1"/>
		<param name="predictspawn" value="0"/>
	</flags>
	<physics type="particle">
		<param name="mass" value="200"/>
		<param name="speed" value="10"/>
		<param name="radius" value="0.125"/>
		<param name="air_resistance" value="0"/>
		<param name="water_resistance" value="0.5"/>
		<param name="gravity" value="0, 0, -9.8"/>
		<param name="water_gravity" value="0, 0, -9.8"/>
		<param name="thrust" value="0"/>
		<param name="lift" value="0"/>
		<param name="min_bounce_speed" value="8"/>
		<param name="material" value="mat_metal"/>
		<param name="spin_random" value="0, 0, 0"/>
		<param name="single_contact" value="1"/>
		<param name="pierceability" value="14"/>
	</physics>
	<explosion>
		<param name="pressure" value="100"/>
		<param name="min_radius" value="4"/>
		<param name="max_radius" value="8"/>
		<param name="min_phys_radius" value="1.5"/>
		<param name="max_phys_radius" value="2"/>
		<param name="hole_size" value="2"/>
		<param name="terrain_hole_size" value="3"/>
		<param name="decal" value="textures/decal/explo_decal.dds"/>
		<param name="effect_scale" value="0.2"/>
		<param name="radialblurdist" value="5"/>
		<param name="max_explode_on_impact_angle" value="181"/>
		<param name="min_explode_on_impact_lifetime" value="0.0"/>
	</explosion>
	<params>
		<param name="lifetime" value="99999"/>
		<param name="showtime" value="0.0"/>
		<param name="bulletType" value="2"/> <!-- Baullet Type 2 - Assault rifle bullet -->
		<param name="giveable" value="1"/>
		<param name="accel" value="10"/>
		<param name="turn_speed" value="150"/>
		<param name="max_speed" value="100"/>
		<param name="cruise_altitude" value="55"/>
		<param name="launch_type" value="0"/>
	</params>
</ammo>
